{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "baseBranches": [
    "develop"
  ],
  "extends": [
    "config:recommended"
  ],
  "labels": [
    "dependencies"
  ],
  "timezone": "Asia/Tokyo",
  "prConcurrentLimit": 10,
  "packageRules": [
    {
      "matchPackagePrefixes": [
        "androidx.paging:paging-common",
        "app.cash.paging:paging-compose-common"
      ],
      "groupName": "PagingLibraries"
    }
  ]
}
